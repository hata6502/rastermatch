{
  "name": "rastermatch",
  "version": "1.0.3",
  "author": "hata6502",
  "type": "module",
  "main": "docs/index.js",
  "bin": {
    "rastermatch": "docs/cli.js"
  },
  "scripts": {
    "build": "tsc && npm run build:css && npm run build:app",
    "build:css": "npx tailwindcss -i src/index.css -o docs/index.css --minify",
    "build:app": "esbuild src/app.tsx --bundle --format=esm --minify --outfile=docs/app.js",
    "start": "http-server -c-1 docs"
  },
  "dependencies": {
    "diff": "^7.0.0",
    "pngjs": "^7.0.0"
  },
  "devDependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@types/diff": "^6.0.0",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "clsx": "^2.1.1",
    "esbuild": "^0.24.0",
    "http-server": "^14.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2"
  }
}
