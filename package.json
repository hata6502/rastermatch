    "start": "http-server -c-1 docs"
  },
  "dependencies": {
    "@types/diff": "^6.0.0",
    "diff": "^7.0.0",
    "pngjs": "^7.0.0"
  },
  "devDependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^18.3.16",
    "@types/react-dom": "^18.3.5",
    "clsx": "^2.1.1",
    "esbuild": "^0.25.0",
    "http-server": "^14.1.1",
    "pdfjs-dist": "^4.9.155",
    "react": "^18.3.1",
    "react-controlled-translation": "^1.0.20",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2"
  }
}
